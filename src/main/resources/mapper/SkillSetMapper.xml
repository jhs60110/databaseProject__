<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.user.SkillSetMapper">
    <select id="getSkillSetInfo" resultType="com.example.demo.model.user.SkillSetDto" parameterType="String">
        SELECT *
          FROM SKILL_SET
         WHERE e_id = #{e_id}
    </select>

</mapper>
